    <DateTimeAttribute dateName="dateThru" timeName="timeThru" table="Tour__c" column="End_Date_Time__c" />
    <Attribute name="id" table="Tour__c" column="Name" />
    <Attribute name="tmgMetaPKey" table="Tour__c" column="Tour_Template__c" />
