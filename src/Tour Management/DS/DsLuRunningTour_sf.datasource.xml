        </SimpleJoin>
      </Join>
    </Entity>
  </Entities>
  <QueryCondition><![CDATA[
    Tour__c.Default_Driver__c = '#UserPKey#' AND Tour__c.Status__c = 'Running'
  ]]></QueryCondition>
  <OrderCriteria></OrderCriteria>
  <Parameters></Parameters>
</DataSource>